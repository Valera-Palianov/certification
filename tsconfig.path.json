{
  "compilerOptions": {
    "experimentalDecorators": true,
      "baseUrl": ".",
      "paths": {
          "@components/*": ["src/components/*"],
          "@pages/*": ["src/pages/*"],
          "@redux/*": ["src/redux/*"],
          "@utils/*": ["src/utils/*"],
          "@styles/*": ["src/styles/*"],
          "@mixins/*": ["src/styles/mixins*"],
          "@api/*": ["src/api/*"],
          "@/*": ["src/*"]
      }
  }
}
